{
  "name": "facodi-monynha",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "validate": "npm run lint && npm run typecheck && npm run test -- -u",
    "prepare": "husky install",
    "postinstall": "husky install",
    "env:check": "tsx scripts/env-check.ts",
    "i18n:extract": "tsx scripts/i18n-extract.ts",
    "depcheck": "depcheck --ignores=\"@secretlint/secretlint-rule-preset-recommend,@vitest/coverage-v8,autoprefixer,lighthouse,postcss\"",
    "knip": "knip",
    "unimported": "unimported",
    "ts-prune": "ts-prune",
    "analyze:deps": "npm run depcheck && npm run knip && npm run unimported && npm run ts-prune",
    "secretlint": "secretlint \"**/*\"",
    "lighthouse": "node scripts/run-lighthouse.mjs"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tooltip": "1.0.7",
    "@supabase/supabase-js": "2.57.4",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.461.0",
    "next": "14.2.32",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.3.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@secretlint/secretlint-rule-preset-recommend": "8.2.1",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "14.2.2",
    "@types/node": "20.16.11",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.20",
    "depcheck": "1.4.7",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.32",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "9.1.6",
    "jsdom": "24.1.0",
    "knip": "5.33.3",
    "lighthouse": "12.8.2",
    "lint-staged": "15.2.10",
    "playwright": "1.48.2",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "secretlint": "8.2.1",
    "tailwindcss": "3.4.14",
    "ts-prune": "0.10.3",
    "tsx": "4.20.5",
    "typescript": "5.6.3",
    "unimported": "1.28.0",
    "vite": "6.3.6",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "ts-prune": {
    "project": "tsconfig.json",
    "ignore": "(src/app/|\\.next/types|playwright\\.config\\.ts|tailwind\\.config\\.ts|vitest\\.config\\.ts)"
  }
}
